{{define "title"}}
    Gogo Blog
{{end}}

{{define "main"}}
    <div class="col-md-9 blog-main">
        {{range $key,$article := .Articles}}
            <div class="blog-post bg-white p-5 rounded shadow mb-4">
                <h3 class="blog-post-title"><a class="text-dark text-decoration-none">{{ $article.Title }}</a></h3>

                <hr>
                {{ $article.Body }}

            </div>
        {{end}}
    </div>
{{end}}